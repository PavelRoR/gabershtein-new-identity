#get
	padding 200px 0 100px
	background: url(../img/backs/get.png) no-repeat center
	background-size: cover
	margin-bottom: 300px
	position relative
	@media screen and (max-width 560px)
		padding 90px 0
	@media screen and (max-width 386px)
		padding 60px 0
.general-img-get
	width 100%
	bottom: -40%
	left 0
	right 0
	display block
	margin 0 auto
	z-index 200
	@media screen and (max-width 1199px)
		top -10%
	@media screen and (max-width 991px)
		display none
.get-container
	background: url(../img/images/get.png) no-repeat center
	background-size contain
	padding 80px 180px
	width 707px
	@media screen and (max-width 1199px)
		position relative
		left -100px
	@media screen and (max-width 991px)
		left 0
		right 0
		margin 0 auto
	@media screen and (max-width 767px)
		background: #B0CBAA
		border-radius: 40px
		width 100%
		padding 20px
		margin-bottom: 35px
.general-title-get
	margin-top: 200px
	@media screen and (max-width 991px)
		margin-top: 0
@media screen and (max-width 991px)
	.button-wrapper-get
		text-align center